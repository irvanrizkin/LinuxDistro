<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.linuxdistro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".About"></activity>
        <activity android:name=".Tails" />
        <activity android:name=".Ubermix" />
        <activity android:name=".ElementaryOS" />
        <activity android:name=".Debian" />
        <activity android:name=".SteamOS" />
        <activity android:name=".KaliLinux" />
        <activity android:name=".UbuntuMATE" />
        <activity android:name=".CentOS" />
        <activity android:name=".LinuxMint" />
        <activity android:name=".Ubuntu" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>